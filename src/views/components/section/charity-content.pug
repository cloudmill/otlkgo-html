                    +mark-text-svg.charity-cards-svg
                    p.text.charity-cards__text!=item 
        .charity-cards__item 
          +column-top-svg.charity-cards__decor
          .charity-cards__row
            .col 
              h3.charity-cards__title Казахстан
            .col
              p.charity-cards__number.desktop-m 109 млн ₽
          .charity-cards__row
            .col 
              p.charity-cards__description.description.mt-0-m Помощь детям
